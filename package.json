{
  "name": "@travi/eslint-config",
  "description": "Shareable config for ESLint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^14.17.0 || >=16"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/eslint-config",
  "bugs": "https://github.com/travi/eslint-config/issues",
  "homepage": "https://npm.im/@travi/eslint-config",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:engines": "ls-engines"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@form8ion/eslint-config": "5.0.23"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.9.18",
    "commitlint-config-travi": "1.4.23",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "ls-engines": "0.9.0",
    "npm-run-all": "4.1.5",
    "remark-cli": "11.0.0",
    "remark-preset-lint-travi": "4.0.2"
  }
}