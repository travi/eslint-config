{
  "name": "@travi/eslint-config",
  "description": "Shareable config for ESLint",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "engines": {
    "node": "^18.17 || >=20.6.1"
  },
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "repository": "travi/eslint-config",
  "bugs": "https://github.com/travi/eslint-config/issues",
  "homepage": "https://npm.im/@travi/eslint-config",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint .",
    "lint:md": "remark . --frail",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:engines": "ls-engines",
    "lint:publish": "publint --strict"
  },
  "files": [
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "@form8ion/eslint-config": "7.0.1"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.10.1",
    "commitlint-config-travi": "1.4.36",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "ls-engines": "0.9.0",
    "npm-run-all2": "6.1.1",
    "publint": "0.2.4",
    "remark-cli": "12.0.0",
    "remark-preset-lint-travi": "6.0.3"
  }
}
